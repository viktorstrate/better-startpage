    "mathjs": "^5.4.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "parcel-plugin-web-extension": "^1.5.0",
    "web-ext": "^2.9.3"
  }
}
