{
  "name": "better-startpage",
  "version": "1.0.0",
  "description": "A better startpage for your browser",
  "main": "index.js",
  "repository": "git@github.com:viktorstrate/better-startpage.git",
  "author": "viktorstrate <viktorstrate@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "parcel watch src/manifest.json",
